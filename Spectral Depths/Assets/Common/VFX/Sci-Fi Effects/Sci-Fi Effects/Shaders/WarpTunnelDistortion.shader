				float4 screenPos = IN.ase_texcoord3;
				float4 ase_grabScreenPos = ASE_ComputeGrabScreenPos( screenPos );
				float4 ase_grabScreenPosNorm = ase_grabScreenPos / ase_grabScreenPos.w;
				float2 appendResult3 = (float2(_UTile , _VTile));
				float2 uv0_Normal = IN.ase_texcoord4.xy * _Normal_ST.xy + _Normal_ST.zw;
				float2 panner2 = ( ( _TimeParameters.x ) * appendResult3 + uv0_Normal);
				float4 fetchOpaqueVal19 = float4( SHADERGRAPH_SAMPLE_SCENE_COLOR( ( ase_grabScreenPosNorm + float4( ( UnpackNormalScale( tex2D( _Normal, panner2 ), _Scale ) * _Add ) , 0.0 ) ).xy ), 1.0 );
				
				float smoothstepResult36 = smoothstep( 0.0 , 1.0 , ( ( distance( _WorldSpaceCameraPos , WorldPosition ) - _FadeMin1 ) / ( _FadeMax1 - _FadeMin1 ) ));
				
				float3 BakedAlbedo = 0;
				float3 BakedEmission = 0;
				float3 Color = fetchOpaqueVal19.rgb;
				float Alpha = ( IN.ase_color.a * ( 1.0 - smoothstepResult36 ) * _Alpha );
				float AlphaClipThreshold = 0.5;

				#ifdef _ALPHATEST_ON
