					#elif defined(MAIN_LIGHT_CALCULATE_SHADOWS)
						ShadowCoords = TransformWorldToShadowCoord( WorldPosition );
					#endif
				#endif
				float2 appendResult14 = (float2(_UTile , _VTile));
				float2 uv0_Mask = IN.ase_texcoord3.xy * _Mask_ST.xy + _Mask_ST.zw;
				float2 panner7 = ( ( _TimeParameters.x ) * appendResult14 + uv0_Mask);
				float4 tex2DNode17 = tex2D( _Mask, panner7 );
				float4 lerpResult18 = lerp( ( _TintA * ( 100.0 * _TintA.a ) ) , ( _TintB * ( 100.0 * _TintB.a ) ) , tex2DNode17);
				float4 temp_cast_0 = (_Exp).xxxx;
				float smoothstepResult40 = smoothstep( 0.0 , 1.0 , ( ( distance( _WorldSpaceCameraPos , WorldPosition ) - _FadeMin ) / ( _FadeMax - _FadeMin ) ));
				
				float3 BakedAlbedo = 0;
				float3 BakedEmission = 0;
				float3 Color = ( max( float4( 0,0,0,0 ) , pow( lerpResult18 , temp_cast_0 ) ) * IN.ase_color * IN.ase_color.a * tex2DNode17 * _Boost * ( 1.0 - smoothstepResult40 ) ).rgb;
